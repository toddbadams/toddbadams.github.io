<div id="tree-depth"></div>
<ul id="myTab" class="nav nav-tabs">
    <li><a href="#generate-tab" id="generate-tab-btn" data-toggle="tab">Generate</a></li>
    <li><a href="#keys-tab" id="keys-tab-btn" data-toggle="tab">Keys</a></li>
    <li><a href="#graph-tab" id="graph-tab-btn" data-toggle="tab">Graph</a></li>
    <li><a href="#degenerate-tab" id="degenerate-tab-btn" data-toggle="tab">Degenerate</a></li>
</ul>
<div id="myTabContent" class="tab-content">
    <div class="tab-pane fade in active" id="generate-tab">
        @Html.Partial("_GenerateTab")
    </div>
    <div class="tab-pane fade" id="keys-tab">
        <div id="keys-holder" class="container"></div>
    </div>
    <div class="tab-pane fade" id="graph-tab">
        <canvas style="width: 100%; height: 100%;" id="canvas"></canvas>
    </div>
    <div class="tab-pane fade" id="degenerate-tab">      
        @Html.Partial("_DeGenerateTab")
    </div>
</div>
